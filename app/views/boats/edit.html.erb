<%= form_for @boat do |f| %>
	<h4> Boat Name </h4>
	<%= f.text_field :name %>
	<h4> Location </h4>
	<%= f.select :location, ["Los Angeles", "Portland", "Seattle", "New York", "Miami", "Philadelphia", "Boston", "Baltimore", "Charlseston", "Norfolk", "Houston", "New Orleans", "Detroit", "Dallas", "Columbus", "Charlotte", "Atlanta"	] %>
	<h4> Containers Available </h4>
	<%= f.number_field :max_cont %>
	<%= f.submit %>
<% end %>

